function [ Ms, Vs, Fs, Ds, Sus, Sps ] = giveMeAll( Su, Sp, allF, allD, Mnx, Mny, nx, ny)
%vytvori rozdelene matice a vektory, pro kazdou matici vrati Fka Dcka a
%zdroje ktere budou potreba pro vytvoreni rovnic
numberOfMatrices = Mnx*Mny;
matriceSizeX = nx/Mnx;
matriceSizeY = ny/Mny;
for i=1:Mnx
    for j=1:Mny
        if i == 1
            Mx = matriceSizeX;
            kj = 0;
        else
            Mx = matriceSizeX+1;
            kj = 1;
        end
        if j == 1
            My = matriceSizeY;
            ki = 0;
        else
            My = matriceSizeY+1;
            ki = 1;
        end
        Ms(i).indexes = sparse(Mx*My, Mx*My);
        Vs(i).indexes = zeros(Mx*My, 1);
        i
        j
        Fs(i).indexes = allF((j-1)*matriceSizeX+1-kj:j*matriceSizeX, (i-1)*matriceSizeY+1-ki:i*matriceSizeY); 
        Ds(i).indexes = allD((j-1)*matriceSizeX+1-kj:j*matriceSizeX, (i-1)*matriceSizeY+1-ki:i*matriceSizeY);
        
  
        Sus(i).indexes = [zeros(Mx, Mx-matriceSizeX) ,Su((j-1)*Mx+1:j*Mx, (i-1)*My+1:i*My); zeros(My-matriceSizeY, My)];
        Sps(i).indexes = [zeros(Mx, Mx-matriceSizeX) ,Sp((j-1)*Mx+1:j*Mx, (i-1)*My+1:i*My); zeros(My-matriceSizeY, My)];
    end
end
end

